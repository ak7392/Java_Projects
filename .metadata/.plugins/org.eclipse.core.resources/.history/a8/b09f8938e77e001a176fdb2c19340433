		arr[i] = i+1;
		System.out.println(arr[i]);
	}
	
	ArrayList<Integer> hm = new ArrayList<Integer>();
	hm.add(3);
	hm.add(1);
	hm.add(3);
	
	System.out.println(hm);
	
	Iterator i1 = hm.iterator();
	while (i1.hasNext()) {
		int n1 = (Integer) i1.next();
		System.out.println(n1);
			
		}
	
