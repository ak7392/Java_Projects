package PatternUsingForLoops;

public class DiamondNumericPattern {
	public static void main(String args[]) {
		int n =5; 
		
		for (int i=0; i <= 5; i++) {
			for(int j =0; j<= i; j++) {
				System.out.print(" ");
			}
			for(int k=i; k <= 5; k++) {
				System.out.print("* ");
			}
			System.out.println();
		}
	}

}
