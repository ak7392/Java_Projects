package LeetCode;

public class ListNode{
    int val;
   ListNode next;
	ListNode(int x) { val = x; }
	}

	

public class MiddkeElemntofList {
	
		class Solution {
		    public ListNode middleNode(ListNode head) {
		      ListNode a = head; 
		      ListNode b = head; 
		      
		        while (b != null && b.next != null ){
		            b = b.next.next;
		            a = a.next;
		            
		        }
		        return a;
		    }
		}
}
